{
  "name": "@easrng/elements",
  "version": "0.1.0-beta-0",
  "description": "a tiny framework powered by @preact/signals-core",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./debug": {
      "types": "./dist/debug.d.ts",
      "default": "./dist/debug.js"
    }
  },
  "scripts": {
    "build": "del-cli dist && npm run rollup-ts src/rollup.config.ts",
    "node-ts": "node --import \"data:text/javascript,import{register}from'node:module';import{pathToFileURL}from'node:url';register('ts-node/esm',pathToFileURL('./'))\"",
    "rollup-ts": "npm run node-ts ./node_modules/rollup/dist/bin/rollup -- -c",
    "prepublish": "run-s lint build",
    "lint": "run-p lint:*",
    "lint:xo": "xo",
    "lint:tsc": "tsc",
    "fmt": "run-p fmt:*",
    "fmt:ts": "xo --fix",
    "fmt:json": "prettier -w *.json"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@preact/signals-core": "^1.5.1"
  },
  "devDependencies": {
    "@fastify/pre-commit": "^2.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "del-cli": "^5.1.0",
    "htm": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.9.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "xo": "^0.56.0"
  },
  "author": "easrng",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easrng/elements.git"
  },
  "bugs": {
    "url": "https://github.com/easrng/elements/issues"
  },
  "homepage": "https://github.com/easrng/elements#readme",
  "pre-commit": "lint"
}
